  'event': 'addToCart',
  'ecommerce': {
    'currencyCode': 'MXN',
    'add': {                                // 'add' actionFieldObject measures.
      'products': [{                        //  adding a product to a shopping cart.
        'name': 'Test product',
        'id': '12345',
        'price': '15.25',
        'brand': 'Momiji',
        'category': 'Apparel',
        'variant': 'Gray',
        'quantity': 1
       }]
    }
  }
});
console.log("Hola");
</script>